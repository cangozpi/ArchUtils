{
  "author": "Justin Wilaby",
  "name": "svg-path-interpolator",
  "version": "0.3.0",
  "license": "MIT",
  "description": "The SVG Path Interpolator produces point data representing interpolated values within an SVG path.",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinwilaby/svg-path-interpolator.git"
  },
  "bin": {
    "svgpi": "./bin/svgpi"
  },
  "engines": {
    "node": ">=14.x"
  },
  "type": "module",
  "exports": {
    ".": {
      "module": "./lib/index.js",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./*": "./lib/*"
  },
  "main": "./lib/index.js",
  "browser": "./lib/index.js",
  "files": [
    "bin",
    "config",
    "lib"
  ],
  "keywords": [
    "svg",
    "points",
    "paths",
    "drawing",
    "interpolate",
    "calculate",
    "animate",
    "b√©zier",
    "bezier",
    "curves",
    "polygon"
  ],
  "scripts": {
    "build": "node import-path-plugin.mjs && node copy-deps.mjs"
  },
  "dependencies": {
    "sax-wasm": "2.2.0"
  },
  "devDependencies": {
    "glob": "^8.0.3",
    "typescript": "^4.7.2"
  }
}
